<!DOCTYPE html>
<!doctype html>
<html lang="en">

<head>
    <title>cmuxser配置</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css"
        integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.4.1/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
        integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js"
        integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6"
        crossorigin="anonymous"></script>
    <style>
        .error {
            color: red;
            font-size: 12px;
        }
        .title{
            text-align: center;
            margin:20px 0;
        }
        h3{
            margin:40px 0 20px;
        }
    </style>
</head>

<body>
    <div class="container">
        <h2 class="title">node-server配置</h2>
        <form class="needs-validation" novalidate action="#">
           
            <div class="container">
                <h3>gitlab配置</h3>
                <div class="row">
                    <div class="form-group col">
                        <label for="gitlabName">用户邮箱</label>
                        <input type="text" class="form-control" value="chenyuanguang@cmcm.com" id="gitlabName" name="gitlabName">
                    </div>
                    <div class="form-group col">
                        <label for="gitlabPassword"">密码</label>
                        <input type="password" class="form-control" value="123456" id="gitlabPassword" name="gitlabPassword">
                    </div>
                </div>
                <div class="form-row">
                    <div class="form-group col">
                        <label for="gitlabUrl">gitlab地址(域名即可)</label>
                        <input type="text" class="form-control" value="gitlab.liebaopay.com" id="gitlabUrl" name="gitlabUrl">
                    </div>
                    <div class="form-group col">
                        <label for="chromeUrl">chrome安装地址</label>
                        <input type="text" class="form-control" value="/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome" id="chromeUrl" name="chromeUrl">
                    </div>
                </div>
            </div>
            <button type="button" class="btn btn-primary" id="pre">上一步</button>
            <button type="submit" class="btn btn-primary" id="next">下一步</button>
            <button type="button" class="btn btn-primary" id="skip">跳过</button>
        </form>

    </div>
    <script src="/js/jquery.validate.min.js"></script>
    <script src="/js/addMethod.js"></script>
    <script>
        $(() => {
            $.extend({
                //将序列化的参数转换为对象
                serializeToObj(str) {
                    var obj = {}
                    str.split("&").map((i) => {
                        var itemarr = i.split("=")
                        obj[itemarr[0]] = itemarr[1]
                    })
                    return obj
                }
            })
            $(".needs-validation").validate({
                rules: {
                    gitlabName: {
                        required: true
                    },
                    gitlabPassword:{
                        required: true
                    },
                    gitlabUrl:{
                        required: true
                    },
                    chromeUrl:{
                        required: true
                    }
                },
                messages: {
                    gitlabName: { required: "用户名不能为空" },
                    gitlabPassword: { required: "密码不能为空" },
                    gitlabUrl: { required: "域名不能为空" },
                    chromeUrl: { required: "chrome应用地址不能为空" }
                },
                submitHandler: function (form, event) {
                    event.preventDefault()
                    //验证通过后进行注册
                    next();
                }
            });
            function next() {
               sessionStorage.setItem("gitlabConfig",JSON.stringify($.serializeToObj($(".needs-validation").serialize())))
               window.location.replace("/view/centry")
            }
           
            $("#pre").click(()=>{
                window.location.replace("/view/index")
            })
            $("#skip").click(()=>{
                window.location.replace("/view/centry")
            })
           
        })

    </script>
</body>

</html>